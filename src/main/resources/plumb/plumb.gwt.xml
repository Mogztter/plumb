<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name="com.google.gwt.user.User" />
	<source path="client"/>
	<source path="shared"/>
	
	<generate-with class="plumb.generator.DriverDelegateGenerator">
		<when-type-assignable class="plumb.client.display.ui.DriverDelegate" />
	</generate-with>
	
	<define-property name="plumb.scan.package" values="plumb"/>
	
	<entry-point class="plumb.client.Plumb"/>
</module>